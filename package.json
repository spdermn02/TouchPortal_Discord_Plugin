{
  "name": "tpdiscord",
  "version": "2.0.0",
  "description": "Touch Portal Plugin for Discord using RPC",
  "bin": "src/index.js",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "pkg": "pkg .",
    "pkg-win": "pkg --targets node12-win-x64 .",
    "pkg-mac": "pkg --targets node12-macos-x64 ."
  },
  "author": "Jameson Allen aka Spdermn02",
  "license": "ISC",
  "pkg": {
    "assets": [
      "src/**/*",
      "src/public/scripts/**/*",
      "src/public/images/**/*",
      "src/public/styles/**/*"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "discord-rpc": "^3.1.3",
    "express": "^4.17.1",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.1",
    "out-url": "^1.1.1",
    "touchportal-api": "^1.0.6"
  }
}
